{
  "manifest_version": 2,
  "name": "Quick Notes",
  "version": "1.0",
  "description": "Extensión para tomar notas rápidas en Chrome",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
