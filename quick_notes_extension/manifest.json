{
  "manifest_version": 2,
  "name": "AA Quick Notes V1",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "popup.html",
    "popup.css",
    "popup.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';  script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs='"
}
